#include <iostream>
using namespace std;

int main() {
    int n, a = 1, b = 1, c;
    cout << "Posicion de Fibonacci: ";
    cin >> n;
    
    if(n == 1 || n == 2) {
        cout << "F(" << n << ") = 1" << endl;
    } else {
        for(int i = 3; i <= n; i++) {
            c = a + b;
            a = b;
            b = c;
        }
        cout << "F(" << n << ") = " << b << endl;
    }
    return 0;
}
