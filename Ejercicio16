#include <stdio.h>
int main() {
    int n, i;
    int esPrimo = 1;
    printf("Numero: ");
    scanf("%d", &n);
    if(n < 2) esPrimo = 0;
    else {
        for(i = 2; i*i <= n; i++) {
            if(n % i == 0) {
                esPrimo = 0;
                break;
            }
        }
    }
    if(esPrimo) printf("%d ES primo\n", n);
    else printf("%d NO es primo\n", n);
    return 0;
}
