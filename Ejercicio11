#include <stdio.h>
int mcd(int a, int b) {
    int temp;
    while(b != 0) {
        temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}
int main() {
    int a, b;
    printf("Numero 1: "); scanf("%d", &a);
    printf("Numero 2: "); scanf("%d", &b);
    printf("MCD = %d\n", mcd(a, b));
    return 0;
}
